<base:SessionWindow x:Class="SEIDR.WindowMonitor.HelperWindows.TeamChooser"
        xmlns:base="clr-namespace:SEIDR.WindowMonitor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEIDR.WindowMonitor.HelperWindows"
        mc:Ignorable="d" Background="LightSteelBlue"
        Title="TeamChooser" Height="110" Width="300">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom">
            <Button Name="OkB" Click="OkB_Click" Margin="30,5,5,5" Width="80">OK</Button>
            <Button Name="CancelB" Click="CancelB_Click" Margin="10,5,5,5" Width="80">Cancel</Button>
        </StackPanel>
        <StackPanel DockPanel.Dock="Top" Margin="15,15,15,0">
            <ComboBox Name="TeamBox" IsReadOnly="True"></ComboBox>
        </StackPanel>
    </DockPanel>
</base:SessionWindow>
