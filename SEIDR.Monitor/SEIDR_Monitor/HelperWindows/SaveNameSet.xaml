<Window x:Class="SEIDR.WindowMonitor.SaveNameSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Set Package File Save name" Height="120" Width="300" WindowStartupLocation="CenterOwner"
        Background="LightSteelBlue">
    <DockPanel Margin="10" LastChildFill="False">
        <TextBox Name="FileName" DockPanel.Dock="Top" Height="20"></TextBox>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom">
            <Button Name="Accept" Click="Accept_Click" Margin="60,10">Accept</Button>
            <Button Name="Cancel" Click="Cancel_Click" Margin="0,10">Cancel</Button>
        </StackPanel>
    </DockPanel>
</Window>
