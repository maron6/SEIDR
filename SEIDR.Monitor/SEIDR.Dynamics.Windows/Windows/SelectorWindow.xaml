<session:BasicSessionWindow x:Class="SEIDR.Dynamics.Windows.SelectorWindow"
        xmlns:session ="clr-namespace:SEIDR.Dynamics.Configurations"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEIDR.Dynamics.Windows"
        mc:Ignorable="d"
        Title="SelectorWindow" Height="400.465" Width="372.558" MinHeight="400" MinWidth="300">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom">
            <Button Name="OK" Click="OK_Click" Width="55" Margin="5,5">OK</Button>
            <Button Name="Cancel" Click="Cancel_Click" Width="55" Margin="5,5">Cancel</Button>
        </StackPanel>
        <ListBox Name="Data" SelectedItem="{Binding Selection}"
                 MouseDoubleClick="Data_MouseDoubleClick"                  
                 Margin="10,10,10,0"
                 >
        </ListBox>
    </DockPanel>
</session:BasicSessionWindow>
